{
  "files": [],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@schemas": [
        "src/app/schemas"
      ],
      "@types": [
        "src/app/types"
      ],
      "@hooks": [
        "src/app/hooks"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@actions": [
        "src/store/actions"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@styles/*": [
        "src/shared/styles/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@/*": [
        "src/*"
      ]
    },
  },
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ]
}